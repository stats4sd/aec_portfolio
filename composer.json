{
    "name": "stats4sd/laravel-template",
    "type": "project",
    "description": "A Laravel Data Platform Template.",
    "keywords": ["framework", "laravel", "template"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "backpack/crud": "^5.5.5",
        "backpack/permissionmanager": "^6.0",
        "backpack/pro": "*",
        "doctrine/dbal": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^10.0",
        "laravel/telescope": "^4.14",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "stats4sd/laravel-backpack-section-title": "dev-backpack-5",
        "stats4sd/laravel-file-util": "dev-laravel-9-support",
        "staudenmeir/belongs-to-through": "^2.12",
        "symfony/http-client": "^6.2",
        "symfony/mailgun-mailer": "^6.1"
    },
    "require-dev": {
        "backpack/generators": "^3.3",
        "barryvdh/laravel-ide-helper": "^2.10",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.4",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.backpackforlaravel.com/"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
